<p id="notice"><%= notice %></p>

<h1>Tech Stacks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tech_stacks.each do |tech_stack| %>
      <tr>
        <td><%= link_to 'Show', tech_stack, class: 'btn btn-primary' %></td>
        <td><%= link_to 'Edit', edit_techstack_path(tech_stack), class: 'btn btn-warning' %></td>
        <td><%= link_to 'Destroy', tech_stack, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tech Stack', new_techstack_path %>
