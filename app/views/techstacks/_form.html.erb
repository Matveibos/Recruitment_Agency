<%= form_with(model: tech_stack) do |form| %>
  <%= render "shared/error_messages", resource: tech_stack %>

  <div class="mb-3">
    <%= form.label :name,class:'form-label' %>
    <%=form.text_field :name, class:'form-control' %>
  </div>

  <div class="mb-3">
    <%= form.label :level,class:'form-label' %>
    <%=form.text_field :level, class:'form-control' %>
  </div>

  <div class="mb-3">
    <%= form.label :entity_id,class:'form-label' %>
    <%=form.text_field :entity_id, class:'form-control', :readonly => true %>
  </div>

  <div class="mb-3">
    <%= form.label :entity_type,class:'form-label' %>
    <%=form.text_field :entity_type, class:'form-control', :readonly => true %>
  </div>

  <div class="actions">
    <%= form.submit 'Add stack', class:'btn btn-primary'%>
  </div>
<% end %>
